			<item id = "1028" text = "浏览..."/>
			<item id = "1029" text = "下载"/>
			<item id = "1030" text = "取消"/>
			<item id = "1031" text = "全选"/>
			<item id = "1032" text = "反选"/>
			<FileList>
				<FileName text = "文件名"/>
				<FileSize text = "大小"/>
			</FileList>
		</NewTask>
		<Config title = "配置">
			<item id = "1036" text = "消息提示"/>
			<item id = "1037" text = "主窗口颜色"/>
			<item id = "1038" text = "退出时提示有下载任务"/>
			<item id = "1039" text = "提示有未完成下载任务"/>
