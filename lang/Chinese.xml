	<UI title = "NOKIA固件下载器">
		<Product text = "产品型号："/>
		<Release text = "发布版本："/>
		<Variant text = "CODE："/>
		<FileList label = "文件列表：">
			<FileName text = "文件名"/>
			<FileSize text = "大小"/>
		</FileList>
		<Download text = "添加到下载任务"/>
	</UI>
	<TaskMgr title = "下载任务">
		<ListView>
			<Status text = "状态"/>
			<FileName text = "文件名"/>
			<FileSize text = "大小"/>
			<Progress text = "进度"/>
			<Speed text = "速度"/>
		</ListView>
	</TaskMgr>
	<Dialog>
		<CODE title = "请输入CODE"/>
		<About title = "关于"/>
